<p id="notice"><%= notice %></p>

<h1>Coffee Tables</h1>

<table>
  <thead>
    <tr>
      <th>Room</th>
      <th>Available</th>
      <th>Description</th>
      <th>Privilege</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coffee_tables.each do |coffee_table| %>
      <tr>
        <td><%= coffee_table.room %></td>
        <td><%= coffee_table.available %></td>
        <td><%= coffee_table.description %></td>
        <td><%= coffee_table.privilege %></td>
        <td><%= link_to 'Show', coffee_table %></td>
        <td><%= link_to 'Edit', edit_coffee_table_path(coffee_table) %></td>
        <td><%= link_to 'Destroy', coffee_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coffee Table', new_coffee_table_path %>
